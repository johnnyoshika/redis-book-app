@model List<RedisBookApp.Book>

<html>
<head>
    <title></title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var book in Model)
            {
            <tr>
                <td>@book.Id</td>
                <td>@book.Title</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>